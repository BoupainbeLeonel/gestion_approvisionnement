{% extends 'base.html.twig' %}

{% block title %}Approvisionnement{% endblock %}

{% block body %}
    <h1>Approvisionnement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ approvisionnement.id }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ approvisionnement.reference }}</td>
            </tr>
            <tr>
